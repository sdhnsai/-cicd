pipeline{
    agent any
    environment{
       PATH="/opt/apache-maven-3.6.3/bin:$PATH"
    }
     stages{
        stage("welcome"){
            steps{
                git 'https://github.com/sdhnsai/myweb'
            }
        }
        stage("maven build"){
           steps{
              sh "mvn clean package"
    }
}
